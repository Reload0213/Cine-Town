-- 테이블 cgv.cart 구조 내보내기
CREATE TABLE IF NOT EXISTS `cart` (
  `cartNum` int(11) NOT NULL AUTO_INCREMENT,
  `cartAmount` int(11) DEFAULT NULL,
  `cartMoney` int(11) DEFAULT NULL,
  `cartIsdelete` varchar(50) DEFAULT 'N',
  `userNum` int(11) DEFAULT NULL,
  `gdsNum` int(11) DEFAULT NULL,
  `gdsPrice` int(11) DEFAULT NULL,
  `gdsName` varchar(50) DEFAULT NULL,
  `gdsImage1` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`cartNum`),
  KEY `FK1_userNum` (`userNum`),
  KEY `FK2_gdsNum` (`gdsNum`),
  CONSTRAINT `FK1_userNum` FOREIGN KEY (`userNum`) REFERENCES `user` (`userNum`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `FK2_gdsNum` FOREIGN KEY (`gdsNum`) REFERENCES `goods` (`gdsNum`) ON DELETE NO ACTION ON UPDATE NO ACTION

) ENGINE=InnoDB AUTO_INCREMENT=31 DEFAULT CHARSET=utf8mb3 COMMENT='장바구니';


-- 테이블 데이터 cgv.cart:~20 rows (대략적) 내보내기
DELETE FROM `cart`;
/*!40000 ALTER TABLE `cart` DISABLE KEYS */;
