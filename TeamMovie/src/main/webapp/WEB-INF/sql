/*!40000 ALTER TABLE `admin` ENABLE KEYS */;

-- 테이블 cgv.cart 구조 내보내기
CREATE TABLE IF NOT EXISTS `cart` (
  `cartNum` int(11) NOT NULL AUTO_INCREMENT,
  `cartAmount` int(11) DEFAULT NULL,
  `cartTotal` int(11) DEFAULT NULL,
  `cartIsdelete` varchar(255) DEFAULT 'n',
  PRIMARY KEY (`cartNum`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COMMENT='장바구니';

-- 테이블 데이터 cgv.cart:~0 rows (대략적) 내보내기
