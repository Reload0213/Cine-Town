-- 테이블 cgv.movie 구조 내보내기
CREATE TABLE IF NOT EXISTS `movie` (
  `mvNum` int(11) NOT NULL AUTO_INCREMENT,
  `mvTitle` varchar(50) DEFAULT NULL,
  `mvActor` varchar(50) DEFAULT NULL,
  `mvContent1` longtext DEFAULT NULL,
  `mvContent2` longtext DEFAULT NULL,
  `mvContent3` longtext DEFAULT NULL,
  `mvContent4` longtext DEFAULT NULL,
  `mvGenre` varchar(50) DEFAULT NULL,
  `mvYear` varchar(50) DEFAULT NULL,
  `mvAge` varchar(50) DEFAULT NULL,
  `mvRuntime` int(11) DEFAULT NULL,
  `mvProducer` varchar(50) DEFAULT NULL,
  `mvAvgRate` decimal(20,6) DEFAULT NULL COMMENT '영화 개인평점 합산',
  `mvIsdelete` varchar(50) DEFAULT 'N',
  `mvPosterPath` varchar(255) DEFAULT NULL,
  `mvBgPath` varchar(255) DEFAULT NULL,
  `mvTrailer` varchar(255) DEFAULT NULL,

  PRIMARY KEY (`mvNum`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8mb3 COMMENT='영화 데이터베이스';

-- 테이블 데이터 cgv.movie:~9 rows (대략적) 내보내기
DELETE FROM `movie`;
/*!40000 ALTER TABLE `movie` DISABLE KEYS */;
