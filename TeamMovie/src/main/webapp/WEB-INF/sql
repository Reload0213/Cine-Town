-- 테이블 cgv.goods 구조 내보내기
CREATE TABLE IF NOT EXISTS `goods` (
  `gdsNum` int(11) NOT NULL AUTO_INCREMENT,
  `gdsPrice` int(11) DEFAULT NULL,
  `gdsStock` int(11) DEFAULT NULL,
  `gdsViewcnt` int(11) DEFAULT NULL,
  `gdsUUID` int(11) DEFAULT NULL,
  `gdsName` varchar(50) DEFAULT NULL,
  `gdsDetail` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin DEFAULT NULL,
  `gdsRegdate` varchar(50) DEFAULT curdate(),
  `gdsIsdelete` varchar(50) DEFAULT 'N',
  `gdsBrand` varchar(50) DEFAULT NULL,
  `gdsImage1` varchar(255) DEFAULT NULL,
  `gdsImage2` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`gdsNum`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8mb3 COMMENT='굿즈 gds';

