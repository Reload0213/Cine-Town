-- 테이블 cgv.reviewreply 구조 내보내기
DROP TABLE IF EXISTS `reviewreply`;
CREATE TABLE IF NOT EXISTS `reviewreply` (
  `rpNum` int(11) NOT NULL AUTO_INCREMENT,
  `rpDate` varchar(50) DEFAULT NULL,
  `rpComment` longtext DEFAULT NULL,
  `rpWritername` varchar(50) DEFAULT NULL,
  `rpLike` int(11) DEFAULT NULL,
  `rpWriternum` int(11) DEFAULT NULL,
  `mvNum` int(11) DEFAULT NULL,
  PRIMARY KEY (`rpNum`),
  KEY `mvNum` (`mvNum`),
  CONSTRAINT `FK_mv` FOREIGN KEY (`mvNum`) REFERENCES `movie` (`mvNum`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8mb3 COMMENT='리뷰댓글\r\n';

