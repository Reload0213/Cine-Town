-- 테이블 cgv.movie 구조 내보내기
CREATE TABLE IF NOT EXISTS `movie` (
  `mvNum` int(11) NOT NULL AUTO_INCREMENT,
  `mvTitle` varchar(255) DEFAULT NULL,
  `mvDirector` varchar(255) DEFAULT NULL,
  `mvDetail` longtext DEFAULT NULL,
  `mvGenre` varchar(255) DEFAULT NULL,
  `mvYear` varchar(255) DEFAULT NULL,
  `mvAge` int(11) DEFAULT NULL,
  `mvRuntime` int(11) DEFAULT NULL,
  `mvProducer` varchar(255) DEFAULT NULL,
  `mvAvgRate` decimal(20,6) DEFAULT NULL COMMENT '개인평점합산해서산출, 별점으로 주면 수식을 다르게 줘야 함',
  `mvIsdelete` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`mvNum`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COMMENT='영화 데이터베이스';

-- 테이블 데이터 cgv.movie:~0 rows (대략적) 내보내기
