/*!40000 ALTER TABLE `cart` ENABLE KEYS */;

-- 테이블 cgv.goods 구조 내보내기
CREATE TABLE `goods` (
	`gdsNum` INT(11) NOT NULL AUTO_INCREMENT,
	`gdsPrice` INT(11) NULL DEFAULT NULL,
	`gdsStock` INT(11) NULL DEFAULT NULL,
	`gdsViewcnt` INT(11) NULL DEFAULT NULL,
	`gdsUUID` INT(11) NULL DEFAULT NULL,
	`gdsName` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb3_general_ci',
	`gdsDetail` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8mb4_bin',
	`gdsRegdate` VARCHAR(50) NULL DEFAULT curdate() COLLATE 'utf8mb3_general_ci',
	`gdsIsdelete` VARCHAR(50) NULL DEFAULT 'n' COLLATE 'utf8mb3_general_ci',
	`gdsBrand` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb3_general_ci',
	`gdsImage1` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb3_general_ci',
	`gdsImage2` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb3_general_ci',
	PRIMARY KEY (`gdsNum`) USING BTREE
)
COMMENT='굿즈 gds'
COLLATE='utf8mb3_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=2
;


-- 테이블 데이터 cgv.goods:~0 rows (대략적) 내보내기
DELETE FROM `goods`;
/*!40000 ALTER TABLE `goods` DISABLE KEYS */;
/*!40000 ALTER TABLE `goods` ENABLE KEYS */;

