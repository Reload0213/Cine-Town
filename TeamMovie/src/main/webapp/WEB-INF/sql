-- 테이블 cgv.admin 구조 내보내기
CREATE TABLE IF NOT EXISTS `admin` (
  `adId` varchar(50) NOT NULL COMMENT 'code 0',
  `adPw` varchar(50) NOT NULL,
  `adName` varchar(50) NOT NULL,
  `verify` int(2) DEFAULT 9
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COMMENT='관리자 데이터';

