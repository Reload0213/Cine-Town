-- 테이블 cgv.goodsreview 구조 내보내기
CREATE TABLE IF NOT EXISTS `goodsreview` (
  `grNum` int(11) NOT NULL AUTO_INCREMENT,
  `grDate` varchar(50) DEFAULT NULL,
  `grComment` longtext DEFAULT NULL,
  `grLike` varchar(50) DEFAULT 'n',
  `grIsdelete` varchar(50) DEFAULT 'n',
  PRIMARY KEY (`grNum`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COMMENT='굿즈리뷰 gr';

